{
  "expo": {
    "name": "Nuveen",
    "slug": "nuveen",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nuveen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NFCReaderUsageDescription": "Nuveen needs NFC access to scan your tag and stop alarms.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio",
          "processing",
          "fetch"
        ]
      },
      "bundleIdentifier": "com.nuveen.alarm",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "android.permission.NFC",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.nuveen.alarm"
    },
    "notification": {
      "color": "#F4C07A"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "color": "#F4C07A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1dc210bc-3d6f-4595-96e8-173e253d1036"
      }
    },
    "owner": "lucas_fs"
  }
}
